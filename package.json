{
  "name": "replacefile-vscode-plugin",
  "displayName": "replaceFile-vscode-plugin",
  "description": "created for file operation",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:replacefile"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "replacefile",
        "title": "replacefile222"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "replacefile",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.67.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2",
    "uint8arrays": "^3.0.0",
    "vsce": "^2.9.1"
  },
  "dependencies": {}
}
